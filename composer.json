{
    "name": "jworksuk/mondo-bank-api",
    "type": "library",
    "description": "PHP Mondo Bank Client",
    "keywords": [
        "jworksuk",
        "mondo-bank-api",
        "mondo-bank"
    ],
    "homepage": "https://github.com/jworksuk/mondo-bank-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse James",
            "email": "me@jworksuk.com",
            "homepage": "http://www.jworksuk.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "symfony/intl": "^3.1",
        "php-http/client-implementation": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "symfony/var-dumper": "^3.1",
        "filp/whoops": "^2.1",
        "php-http/guzzle6-adapter": "^1.1",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "JWorksUK\\Mondo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jworksuk\\Mondo\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "psr2": "phpcs --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
